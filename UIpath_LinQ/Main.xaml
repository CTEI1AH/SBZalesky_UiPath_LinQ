        <OutArgument x:TypeArguments="sd:DataTable">[MergedDT]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="sd:DataTable" xml:space="preserve">[(From row In MergedDT.AsEnumerable()
            Order By DateTime.ParseExact(row.Item("Дата приказа").ToString(), "dd.MM.yy", System.Globalization.CultureInfo.InvariantCulture) Descending
             Select row).CopyToDataTable()]</InArgument>
      </Assign.Value>
    </Assign>
    <Assign DisplayName="Удаление дубликатов" sap:VirtualizedContainerService.HintSize="570,115" sap2010:WorkflowViewState.IdRef="Assign_33">
      <Assign.To>
        <OutArgument x:TypeArguments="sd:DataTable">[MergedDT]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="sd:DataTable" xml:space="preserve">[(From row In MergedDT.AsEnumerable()
         Group By key = New With {
             Key .ProgKod = row.Item("Программа.Кодификатор").ToString(),
             Key .Potok = row.Item("Поток").ToString(),
			 Key .UserID = row.Item("Слушатель.User id").ToString(),
			 Key .YrLico = row.Item("Юр. лицо").ToString(),
			 Key .GroupSF = row.Item("Группа SF.Артикул").ToString()
         } Into Group
         Select Group.First()).CopyToDataTable()]</InArgument>
      </Assign.Value>
    </Assign>
    <Assign DisplayName="Взять только актуальные данные" sap:VirtualizedContainerService.HintSize="570,115" sap2010:WorkflowViewState.IdRef="Assign_34">
      <Assign.To>
        <OutArgument x:TypeArguments="sd:DataTable">[MergedDT]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="sd:DataTable" xml:space="preserve">[(From s As DataRow In MergedDT.AsEnumerable() 
Where  Not String.IsNullOrEmpty(s.Item("Итого").ToString())
Select s).CopyToDataTable()]</InArgument>
      </Assign.Value>
